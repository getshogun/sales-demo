<body data-spy="scroll" data-target=".navigation">
  <%= render 'layouts/header' %>
  <%= yield %>
  <%= render 'layouts/footer' %>

  <!-- Start Scroll To Top  -->
  <a href="#" class="scroll-up"><i class="fa fa-arrow-up"></i></a>
  <!-- End Scroll To Top  -->

  <%= javascript_include_tag "application" %>

